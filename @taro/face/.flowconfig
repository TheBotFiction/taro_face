[ignore]
<PROJECT_ROOT>/node_modules/.*
<PROJECT_ROOT>/dist/.*
<PROJECT_ROOT>/src/stories/.*
<PROJECT_ROOT>/.storybook/.*

[include]
<PROJECT_ROOT>/node_modules/react
<PROJECT_ROOT>/node_modules/prop-types
<PROJECT_ROOT>/node_modules/fbjs/lib
<PROJECT_ROOT>/node_modules/apollo-boost
<PROJECT_ROOT>/node_modules/react-apollo
<PROJECT_ROOT>/node_modules/graphql-tag
<PROJECT_ROOT>/node_modules/immutable
<PROJECT_ROOT>/node_modules/@material-ui/core

[libs]

[lints]

[options]
module.system=haste
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=src
module.name_mapper='^components\(.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^containers\(.*\)$' -> '<PROJECT_ROOT>/src/containers/\1'
module.name_mapper='^types\(.*\)$' -> '<PROJECT_ROOT>/src/types/\1'

[strict]

[version]
^0.79.0